//javascript
selectionSort(arr,n){
   //code here
      let min;
  for (let i = 0; i < n - 1; i++) {
    min = i;
   
    for (let j = i + 1; j < n; j++) {
      if (arr[j] < arr[min]) {
        min = j;
      }
    }
  
  
     const temp = arr[i]
     arr[i] = arr[min]
     arr[min] = temp
  }
  return arr;
  }
