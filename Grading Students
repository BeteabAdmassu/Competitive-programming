function gradingStudents(grades) {
    // Write your code here
   
   for( let i = 0 ; i < grades.length; i++)
    {
     
        if( grades[i] >=  38)
          {
             let nextMul = 5 + ( grades[i] - (grades[i] % 5))
              if(nextMul - grades[i] < 3)
               grades[i] = nextMul
          }
    }
    return grades;
}
