function insertionSort1(n, arr) {
    // Write your code here
  
  let key = arr[n - 1]
   for(let i = n - 2 ; i >= -1 ; i--)
   {
       if(key >= arr[i] && i >= 0)
        {
         arr[i + 1] = key
         console.log(...arr)
         break
         }
       else if( key < arr[i] && i >= 0)
        {
          arr[i + 1] = arr[i]
          console.log(...arr)}
        else
         {
         arr[i + 1 ] = key
         console.log(...arr)
          }
}
}
